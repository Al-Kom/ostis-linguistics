sc_agent_finding_impossible_lexeme_classes_combinations
    => nrel_main_idtf:
        [sc-агент нахождения невозможных комбинаций признаков в классификациях лексем] (* <- lang_ru;; *);
        [sc-agent of finding impossible lexeme classes combinations] (* <- lang_en;; *);

    <- abstract_sc_agent;

    => nrel_primary_initiation_condition:
        (sc_event_add_output_arc => question_initiated);

    => nrel_initiation_condition_and_result:
        (..sc_agent_finding_impossible_lexeme_classes_combinations_initiation_condition => ..sc_agent_finding_impossible_lexeme_classes_combinations_result);

    <= nrel_sc_agent_key_sc_elements:
    {
        question_initiated;
        question;
        question_finding_impossible_lexeme_classes_combinations
    };

    => nrel_inclusion: .platform_independent_realization_of_sc_agent_finding_impossible_lexeme_classes_combinations
    (*
        <- platform_independent_abstract_sc_agent;;
        <= nrel_sc_agent_program:
        {
            agent_finding_impossible_lexeme_classes_combinations
        };;

        -> sc_agent_finding_impossible_lexeme_classes_combinations_scp
            (* <- active_sc_agent;; *);;
    *);;

    ..sc_agent_finding_impossible_lexeme_classes_combinations_initiation_condition
    = [*
        question_finding_impossible_lexeme_classes_combinations _-> .._question;;
        question_initiated _-> .._question;;
        question _-> .._question;;
        .._question _-> .._parameter;;
    *];;

    ..sc_agent_finding_impossible_lexeme_classes_combinations_result
    = [*
        question_finding_impossible_lexeme_classes_combinations _-> .._question;;
        question_finished _-> .._question;;
        question _-> .._question;;
        .._question _=> nrel_answer:: .._answer;;
        .._question _-> .._parameter;;
    *];;

