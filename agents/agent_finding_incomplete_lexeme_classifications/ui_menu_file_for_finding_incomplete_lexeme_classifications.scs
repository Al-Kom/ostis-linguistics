ui_menu_file_for_finding_incomplete_lexeme_classifications
    <- ui_user_command_class_atom; ui_user_command_class_view_kb;
    => nrel_main_idtf:
        [Какие классификации лексем имеют незаконченный вид?] (* <- lang_ru;; *);
        [Which classifications of lexemes are incomplete?] (* <- lang_en;; *);
    => ui_nrel_command_template:
	[*
		question_finding_incomplete_lexeme_classifications _-> ._question_finding_incomplete_lexeme_classifications;;
        ._question_finding_incomplete_lexeme_classifications _<- question;;
    *];
	=> ui_nrel_command_lang_template: 
        [Запрос нахождения незаконченных классификаций лексем] (* <- lang_ru;; *);
        [Request to find incomplete classifications of lexemes] (* <- lang_en;; *);;

