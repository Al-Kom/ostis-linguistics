        ui_user_command_class_view_kb;
    => nrel_main_idtf:
        [Какова лексема данной словоформы?] (* <- lang_ru;; *);
        [What is the lexeme of this word form?] (* <- lang_en;; *);
    => ui_nrel_command_template:
    [*
        question_finding_lexeme_of_word_form _-> ._question_finding_lexeme_of_word_form
        (*
            _-> ui_arg_1;;
        *);;
        ._question_finding_lexeme_of_word_form _<- question;;
    *];
    => ui_nrel_command_lang_template: 
        [Запрос нахождения лексемы словоформы: $ui_arg_1] (* <- lang_ru;; *);
        [Request to find lexeme of word form: $ui_arg_1] (* <- lang_en;; *);;

